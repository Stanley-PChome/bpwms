		<add key="ShareFolderTemp" value="C:\PhWms3pl\File\temp\"  xdt:Locator="Match(key)" xdt:Transform="SetAttributes"/>
		<add key="PositionName" value="A7正式機" xdt:Locator="Match(key)" xdt:Transform="SetAttributes"/>
	</appSettings>

