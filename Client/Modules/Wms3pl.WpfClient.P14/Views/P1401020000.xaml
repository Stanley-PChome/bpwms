                                                            <Binding />
                                                            <Binding RelativeSource="{RelativeSource FindAncestor,AncestorType=DataGrid}" />
                                                        </MultiBinding>
                                                    </DataGridTextColumn.Binding>
                                                </DataGridTextColumn>
                                                <DataGridTextColumn Binding="{Binding Path=INVENTORY_DATE, StringFormat=\{0:yyyy/MM/dd\}}" Header="{x:Static Properties:Resources.QueryInventorySDate}" />
