                                    </Expander>
                                </DockPanel>
                                <DockPanel Dock="Top">
                                    <Expander IsExpanded="True">
                                        <DataGrid Style="{StaticResource DgStyle}" SelectionMode="Single" Height="400" IsReadOnly="False" ScrollViewer.HorizontalScrollBarVisibility="Auto" CellStyle="{StaticResource SingleClickEdit}" ItemsSource="{Binding InventoryDetailData, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" SelectedItem="{Binding SelectInventoryDetailData, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" DockPanel.Dock="Top">
