                                                                <DataGrid.Columns>
                                                                    <DataGridTextColumn Header="{x:Static res:Resources.ItemNumber}">
                                                                        <DataGridTextColumn.Binding>
                                                                            <MultiBinding Converter="{StaticResource DataGridRowNumberConverter}">
                                                                                <Binding />
                                                                                <Binding RelativeSource="{RelativeSource FindAncestor,AncestorType=DataGrid}" />
                                                                            </MultiBinding>
                                                                        </DataGridTextColumn.Binding>
                                                                    </DataGridTextColumn>
                                                                    <DataGridTextColumn Binding="{Binding Path=ProductId}" Header="{x:Static Properties:Resources.ITEM_NAME}" />
                                                                    <DataGridTextColumn Binding="{Binding Path=ProductName}" Header="{x:Static Properties:Resources.ITEM_CODE}" />
                                                                </DataGrid.Columns>
                                                            </DataGrid>
                                                            <Button Content="{x:Static Properties:Resources.ProductId}" controls:PermissionService.FunctionId="BP1401020004" Grid.Column="1" Grid.Row="0" Margin="5" Command="{Binding AddCommand}" />
                                                            <Button Content="{x:Static res:Resources.Delete}" controls:PermissionService.FunctionId="BP1401020005" Grid.Column="1" Grid.Row="1" Margin="5" Command="{Binding DeleteCommand}" VerticalAlignment="Top" />
                                                        </Grid>
                                                    </WrapPanel>
                                                </Grid>
                                            </GroupBox>
                                            <GroupBox Header="{x:Static Properties:Resources.Second_Condition}" Grid.Row="0" Grid.Column="1" Margin="5">
                                                <Grid Margin="5">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="auto" />
                                                        <RowDefinition Height="auto" />
                                                        <RowDefinition Height="auto" />
                                                        <RowDefinition Height="auto" />
                                                    </Grid.RowDefinitions>
                                                    <!--<RadioButton GroupName="giFilter" Content="全部商品" Grid.Column="0" Grid.Row="0" IsChecked="{Binding IsAllProduct, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="10,5,5,0" />-->
                                                    <RadioButton GroupName="giFilter" Content="{x:Static Properties:Resources.AllDifferenceProduct}" Grid.Column="0" Grid.Row="0" Margin="10,5,5,0" IsChecked="{Binding IsDifferenceProduct, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                                                    <WrapPanel Grid.Column="0" Grid.Row="1" Margin="5">
                                                        <RadioButton GroupName="giFilter" Content="{x:Static Properties:Resources.IsRangeProduct}" Margin="5,0,5,0" VerticalAlignment="Center" IsEnabled="{Binding IsDifferenceProduct, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" IsChecked="{Binding IsRangeProduct, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                                                        <TextBox MaxLength="4" MinWidth="45" Margin="0" Text="{Binding RangeStart, Converter={StaticResource StringEmptyToNullConverter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" IsEnabled="{Binding IsRangeProduct}" controls:TextBoxService.NumericOnly="True" />
                                                        <Label Content="{x:Static Properties:Resources.RangeEnd}" />
                                                        <TextBox MaxLength="4" MinWidth="45" Margin="0" Text="{Binding RangeEnd, Converter={StaticResource StringEmptyToNullConverter}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" IsEnabled="{Binding IsRangeProduct}" controls:TextBoxService.NumericOnly="True" />
                                                    </WrapPanel>
                                                </Grid>
                                            </GroupBox>
                                            <GroupBox BorderBrush="Red" Header="{x:Static Properties:Resources.P1401020000_WordingHeader}" Grid.ColumnSpan="2" Grid.Row="1" Grid.Column="0" Margin="5">
                                                <Grid Margin="5">
