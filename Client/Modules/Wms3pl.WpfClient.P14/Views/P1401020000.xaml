                                                        <RowDefinition Height="auto" />
                                                    </Grid.RowDefinitions>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition></ColumnDefinition>
                                                        <ColumnDefinition></ColumnDefinition>
                                                    </Grid.ColumnDefinitions>
                                                    <WrapPanel Grid.Column="0" Grid.Row="0">
                                                        <Label MaxWidth="600">
                                                            <TextBlock FontSize="14" TextWrapping="Wrap" Text="{x:Static Properties:Resources.P1401020000_Wording_1}" />
                                                        </Label>
                                                    </WrapPanel>
                                                    <WrapPanel Grid.Column="1" Grid.Row="0">
                                                        <Label MaxWidth="600">
                                                            <TextBlock FontSize="14" TextWrapping="Wrap" Text="{x:Static Properties:Resources.P1401020000_Wording_2}" />
                                                        </Label>
                                                    </WrapPanel>
                                                </Grid>
                                            </GroupBox>
                                            <WrapPanel Orientation="Horizontal" Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2" Margin="5,0,5,5" HorizontalAlignment="Left">
                                                <Button Content="{x:Static Properties:Resources.SearchDetailData}" Command="{Binding SearchDetailCommand}" controls:PermissionService.FunctionId="BP1401020006" Margin="10,5" Height="30" />
                                                <Button Content="{x:Static Properties:Resources.RegenerateInventoryData}" Command="{Binding RegenerateInventoryCommand}" controls:PermissionService.FunctionId="BP1401020007" Margin="10,5" Height="30" />
                                                <Button Content="{x:Static Properties:Resources.SaveInventoryData}" Command="{Binding SaveInventoryCommand}" controls:PermissionService.FunctionId="BP1401020008" Margin="10,5" Height="30" />
                                                <Button Content="結案" Command="{Binding CaseClosedCommand}" controls:PermissionService.FunctionId="BP1401020009" Margin="10,5" Height="30" />
                                            </WrapPanel>
