                                                <DataGridTextColumn Binding="{Binding Path=MEMO}" Header="{x:Static Properties:Resources.MEMO}" />
                                            </DataGrid.Columns>
                                        </DataGrid>
                                    </Expander>
                                </DockPanel>
                                <DockPanel Dock="Top">
                                    <Expander IsExpanded="True">
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="Auto" />
