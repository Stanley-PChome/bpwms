<?xml version="1.0" encoding="utf-8"?>
<uiLib:Wms3plUserControl x:Class="Wms3pl.WpfClient.P16.Views.P1601050000" xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" mc:Ignorable="d" xmlns:uiLib="clr-namespace:Wms3pl.WpfClient.UILib;assembly=Wms3pl.WpfClient.UILib" xmlns:controls="clr-namespace:Wms3pl.WpfClient.UILib.Controls;assembly=Wms3pl.WpfClient.UILib" xmlns:common="clr-namespace:Wms3pl.WpfClient.Common;assembly=Wms3pl.WpfClient.Common" xmlns:converters="clr-namespace:Wms3pl.WpfClient.Common.Converters;assembly=Wms3pl.WpfClient.Common" xmlns:viewModel="clr-namespace:Wms3pl.WpfClient.P16.ViewModel" xmlns:Properties="clr-namespace:Wms3pl.WpfClient.P16.Properties" xmlns:res="clr-namespace:Wms3pl.WpfClient.Resources;assembly=Wms3pl.WpfClient.Resources" x:Name="Window" Margin="0,0,0,0" HorizontalAlignment="Stretch" FontSize="18" MinWidth="800" controls:PermissionService.FunctionId="P1601050000" controls:PermissionService.FunctionName="退貨報表" FormException="{Binding Exception}">
  <uiLib:Wms3plUserControl.DataContext>
    <viewModel:P1601050000_ViewModel x:Name="Vm" />
  </uiLib:Wms3plUserControl.DataContext>
  <uiLib:Wms3plUserControl.Resources>
    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/Wms3pl.WpfClient.UILib;component/Generic.xaml" />
      </ResourceDictionary.MergedDictionaries>
      <common:NotConverter x:Key="NotConverter" />
      <converters:StringMultiTypeToVisiablityConverter x:Key="StringMultiTypeToVisiablityConverter" />
      <converters:StringTypeToVisiablityConverter x:Key="StringTypeToVisiablityConverter" />
    </ResourceDictionary>
  </uiLib:Wms3plUserControl.Resources>
  <telerik:RadBusyIndicator x:Name="GridBusyIndicator" BusyContent="{x:Static res:Resources.Loading}" IsIndeterminate="False" IsBusy="{Binding IsBusy}">
    <uiLib:BasicMaster x:Name="Master">
      <uiLib:BasicMaster.ToolBarArea>
        <WrapPanel>
          <Button Style="{StaticResource SearchButton}" controls:PermissionService.FunctionId="BP1601050001" Command="{Binding SearchCommand}" Visibility="{Binding ReportTypeForSearch,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged, Converter={StaticResource StringMultiTypeToVisiablityConverter}, ConverterParameter=0;1;4;8}" />
          <Button Style="{StaticResource PreviewButton}" controls:PermissionService.FunctionId="BP1601050002" Command="{Binding PrintCommand}" CommandParameter="{x:Static uiLib:PrintType.Preview}" Visibility="{Binding ReportTypeForSearch,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged, Converter={StaticResource StringMultiTypeToVisiablityConverter}, ConverterParameter=2;3}" />
          <Button Style="{StaticResource PrintButton}" controls:PermissionService.FunctionId="BP1601050003" Command="{Binding PrintCommand}" CommandParameter="{x:Static uiLib:PrintType.ToPrinter}" Visibility="{Binding ReportTypeForSearch,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged, Converter={StaticResource StringMultiTypeToVisiablityConverter}, ConverterParameter=2;3}" />
          <Button Style="{StaticResource ExportButton}" controls:PermissionService.FunctionId="BP1601050004" Command="{Binding ExportCommand}" Visibility="{Binding ReportTypeForSearch,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged, Converter={StaticResource StringMultiTypeToVisiablityConverter}, ConverterParameter=5;6;7}" />
        </WrapPanel>
      </uiLib:BasicMaster.ToolBarArea>
      <uiLib:BasicMaster.MainContent>
        <TabControl x:Name="LayoutRoot">
          <TabControl.BindingGroup>
            <BindingGroup Name="errorGroup" />
          </TabControl.BindingGroup>
          <TabItem Header="{x:Static res:Resources.Main}" Width="Auto">
            <DockPanel>
              <!-- 由此處開始加入畫面項目 -->
              <GroupBox Header="{x:Static res:Resources.QueryCondition}" DockPanel.Dock="Top">
                <Grid>
                  <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="100" />
                    <ColumnDefinition Width="180" />
                    <ColumnDefinition Width="100" />
                    <ColumnDefinition Width="160" />
                    <ColumnDefinition Width="100" />
                    <ColumnDefinition Width="160" />
                  </Grid.ColumnDefinitions>
                  <Grid.RowDefinitions>
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                  </Grid.RowDefinitions>
                  <Label Grid.Row="0" Grid.Column="0" Content="{x:Static Properties:Resources.ReportTypeForSearch}" HorizontalAlignment="Right" Style="{StaticResource LbStyle}" />
                  <ComboBox Style="{StaticResource ComboStyle}" Grid.Row="0" Grid.Column="1" Grid.ColumnSpan="3" Width="430" ItemsSource="{Binding ReportTypeList,UpdateSourceTrigger=PropertyChanged}" SelectedValue="{Binding ReportTypeForSearch,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" SelectedValuePath="Value" DisplayMemberPath="Name" />
                  <Label Grid.Row="1" Grid.Column="0" Content="{x:Static res:Resources.Dc}" HorizontalAlignment="Right" Style="{StaticResource LbStyle}" />
                  <ComboBox Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="3" Width="430" Style="{StaticResource ComboStyle}" ItemsSource="{Binding DcList,UpdateSourceTrigger=PropertyChanged}" SelectedValue="{Binding DcCodeForSearch,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" SelectedValuePath="Value" DisplayMemberPath="Name" />
                  <Label Grid.Row="2" Grid.Column="0" Content="{x:Static Properties:Resources.RETURN_DATE}" HorizontalAlignment="Right" Style="{StaticResource LbStyle}" />
                  <DatePicker Grid.Row="2" Grid.Column="1" SelectedDate="{Binding BeginReturnDate, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Style="{StaticResource DateStyle}" HorizontalAlignment="Left" />
                  <Label Grid.Row="2" Grid.Column="2" Content="~" Style="{StaticResource LbTitleStyle}" />
                  <DatePicker Grid.Row="2" Grid.Column="3" SelectedDate="{Binding EndReturnDate, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Style="{StaticResource DateStyle}" HorizontalAlignment="Left" />
                  <Label Grid.Row="3" Grid.Column="0" Content="{x:Static Properties:Resources.SearchReturn_NO}" Style="{StaticResource LbTitleStyle}" />
                  <TextBox Grid.Row="3" Grid.Column="1" Width="170" VerticalAlignment="Center" Text="{Binding ReturnNoForSearch, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Style="{StaticResource TxtStyle}" MaxLength="20" />
                  <Label Grid.Row="3" Grid.Column="2" Content="{x:Static Properties:Resources.SelectSTATUS}" HorizontalAlignment="Right" Style="{StaticResource LbStyle}" />
                  <ComboBox Grid.Row="3" Grid.Column="3" Style="{StaticResource ComboStyle}" ItemsSource="{Binding StatusList,UpdateSourceTrigger=PropertyChanged}" SelectedValue="{Binding StatusForSearch,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" SelectedValuePath="Value" DisplayMemberPath="Name" />
                </Grid>
              </GroupBox>
              <!-- 退貨報表 -->
              <DataGrid Height="350" DockPanel.Dock="Top" Style="{StaticResource DgStyle}" ItemsSource="{Binding P160201ReportList}" Visibility="{Binding ReportTypeForSearch, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged,              Converter={StaticResource StringTypeToVisiablityConverter}, ConverterParameter=0}">
                <DataGrid.Columns>
                  <DataGridTextColumn Header="{x:Static Properties:Resources.RETURN_DATE}" Binding="{Binding RETURN_DATE, StringFormat={}{0:yyyy/MM/dd}}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.GUP_NAME}" Binding="{Binding GUP_NAME}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.CUST_CODE}" Binding="{Binding CUST_NAME}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.SearchCUST_ORD_NO}" Binding="{Binding CUST_ORD_NO}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.ASCUST_NAME}" Binding="{Binding RETAIL_NAME}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.VNR_CODE}" Binding="{Binding VNR_CODE}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.VNR_NAME}" Binding="{Binding VNR_NAME}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.ITEMCODE}" Binding="{Binding ITEM_CODE}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.ITEMNAME}" Binding="{Binding ITEM_NAME}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.AUDIT_QTY}" Binding="{Binding AUDIT_QTY}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.AREA_NAME}" Binding="{Binding AREA_NAME}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.SearchLoc_Code}" Binding="{Binding LOC_CODE}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.TYPE_NAME}" Binding="{Binding TYPE_NAME}" />
                </DataGrid.Columns>
              </DataGrid>
              <!-- P107_退貨記錄總表報表 -->
              <DataGrid Height="350" DockPanel.Dock="Top" Style="{StaticResource DgStyle}" ItemsSource="{Binding P17ReturnAuditReportList}" Visibility="{Binding ReportTypeForSearch, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged,              Converter={StaticResource StringTypeToVisiablityConverter}, ConverterParameter=1}">
                <DataGrid.Columns>
                  <DataGridTextColumn Header="{x:Static Properties:Resources.ALLOCATE_DATE}" Binding="{Binding RETURN_DATE, StringFormat={}{0:yyyy/MM/dd}}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.SearchCUST_ORD_NO}" Binding="{Binding CUST_ORD_NO}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.CUST_NO}" Binding="{Binding RTN_CUST_CODE}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.ASCUST_NAME}" Binding="{Binding RTN_CUST_NAME}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.MATERIAL_NO}" Binding="{Binding ITEM_CODE}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.ITEMNAME}" Binding="{Binding ITEM_NAME}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.ACCEPTENCE_DATE}" Binding="{Binding CRT_DATE, StringFormat={}{0:yyyy/MM/dd}}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.ALLOCATION_QTY}" Binding="{Binding RTN_QTY}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.REAL_AUDIT_QTY}" Binding="{Binding AUDIT_QTY}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.SHORTFALL}" Binding="{Binding SHORTFALL}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.MULTIRETURN}" Binding="{Binding MULTIRETURN}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.RTN_CAUSE}" Binding="{Binding CAUSE}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.AUDIT_NAME}" Binding="{Binding AUDIT_NAME}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.MODIFY_DATE}" Binding="{Binding UPD_DATE, StringFormat={}{0:yyyy/MM/dd HH:mm:ss}}" />
                </DataGrid.Columns>
              </DataGrid>
              <!-- P106_退貨未上架明細表 -->
              <DataGrid Height="350" DockPanel.Dock="Top" Style="{StaticResource DgStyle}" ItemsSource="{Binding P106ReturnNotMoveDetailList}" Visibility="{Binding ReportTypeForSearch, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged,              Converter={StaticResource StringTypeToVisiablityConverter}, ConverterParameter=4}">
                <DataGrid.Columns>
                  <DataGridTextColumn Header="{x:Static Properties:Resources.AUDIT_DATE}" Binding="{Binding AUDIT_DATE, StringFormat={}{0:yyyy/MM/dd}}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.POSTING_DATE}" Binding="{Binding POSTING_DATE, StringFormat={}{0:yyyy/MM/dd}}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.CUST_NO}" Binding="{Binding RTN_CUST_CODE}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.ASCUST_NAME}" Binding="{Binding RTN_CUST_NAME}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.SearchCUST_ORD_NO}" Binding="{Binding CUST_ORD_NO}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.SearchReturn_NO}" Binding="{Binding RETURN_NO}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.ITEMCODE}" Binding="{Binding ITEM_CODE}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.ITEMNAME}" Binding="{Binding ITEM_NAME}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.APPLICATION_QTY}" Binding="{Binding RTN_QTY}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.ACCEPTENCE_QTY}" Binding="{Binding AUDIT_QTY}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.ALLOW_QTY}" Binding="{Binding ALLOW_QTY}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.NOTALLOW_QTY}" Binding="{Binding NOTALLOW_QTY}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.NOTMOVED_QTY}" Binding="{Binding NOTMOVED_QTY}" />
                </DataGrid.Columns>
              </DataGrid>
              <!-- P015_預計退貨明細表 -->
              <DataGrid Height="350" DockPanel.Dock="Top" Style="{StaticResource DgStyle}" ItemsSource="{Binding P015ForecastReturnDetailList}" Visibility="{Binding ReportTypeForSearch, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged,              Converter={StaticResource StringTypeToVisiablityConverter}, ConverterParameter=8}">
                <DataGrid.Columns>
                  <DataGridTextColumn Header="{x:Static Properties:Resources.CUST_NAME}" Binding="{Binding CUST_NAME}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.WMS_NO}" Binding="{Binding RETURN_NO}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.CUST_ORD_NO}" Binding="{Binding CUST_ORD_NO}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.ASCUST_NO}" Binding="{Binding RTN_CUST_CODE}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.ASCUST_NAME}" Binding="{Binding RTN_CUST_NAME}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.ITEMCODE}" Binding="{Binding ITEM_CODE}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.ITEMNAME}" Binding="{Binding ITEM_NAME}" />
                  <DataGridTextColumn Header="{x:Static Properties:Resources.REAL_RETURN_QTY}" Binding="{Binding RTN_QTY}" />
                </DataGrid.Columns>
              </DataGrid>
            </DockPanel>
          </TabItem>
        </TabControl>
      </uiLib:BasicMaster.MainContent>
    </uiLib:BasicMaster>
  </telerik:RadBusyIndicator>
</uiLib:Wms3plUserControl>