<?xml version="1.0" encoding="utf-8"?>
<uiLib:Wms3plUserControl x:Class="Wms3pl.WpfClient.P05.Views.P0503040000" xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" mc:Ignorable="d" xmlns:uiLib="clr-namespace:Wms3pl.WpfClient.UILib;assembly=Wms3pl.WpfClient.UILib" xmlns:controls="clr-namespace:Wms3pl.WpfClient.UILib.Controls;assembly=Wms3pl.WpfClient.UILib" xmlns:common="clr-namespace:Wms3pl.WpfClient.Common;assembly=Wms3pl.WpfClient.Common" xmlns:Properties="clr-namespace:Wms3pl.WpfClient.P05.Properties" xmlns:res="clr-namespace:Wms3pl.WpfClient.Resources;assembly=Wms3pl.WpfClient.Resources" xmlns:viewModel="clr-namespace:Wms3pl.WpfClient.P05.ViewModel" xmlns:converters="clr-namespace:Wms3pl.WpfClient.Common.Converters;assembly=Wms3pl.WpfClient.Common" xmlns:views="clr-namespace:Wms3pl.WpfClient.UcLib.Views;assembly=Wms3pl.WpfClient.UcLib" x:Name="Window" Margin="0,0,0,0" HorizontalAlignment="Stretch" FontSize="18" MinWidth="800" controls:PermissionService.FunctionId="P0503040000" controls:PermissionService.FunctionName="手動挑單" FormException="{Binding Exception}">
  <uiLib:Wms3plUserControl.DataContext>
    <viewModel:P0503040000_ViewModel x:Name="Vm" />
  </uiLib:Wms3plUserControl.DataContext>
  <uiLib:Wms3plUserControl.Resources>
    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/Wms3pl.WpfClient.UILib;component/Generic.xaml" />
      </ResourceDictionary.MergedDictionaries>
      <common:NotConverter x:Key="NotConverter" />
      <converters:DataGridRowNumberConverter x:Key="DataGridRowNumberConverter" />
      <converters:DateTimeToStringConverter x:Key="DateTimeToStringConverter" />
      <converters:EnumToVisibilityConverter x:Key="EnumToVisibilityConverter" />
      <converters:NameValuePairMultiConverter x:Key="NameValuePairMultiConverter" />
      <FrameworkElement x:Key="ProxyElement" DataContext="{Binding}"></FrameworkElement>
    </ResourceDictionary>
  </uiLib:Wms3plUserControl.Resources>
  <telerik:RadBusyIndicator x:Name="GridBusyIndicator" BusyContent="{x:Static res:Resources.Loading}" IsIndeterminate="False" IsBusy="{Binding IsBusy}">
    <uiLib:BasicMaster x:Name="Master">
      <uiLib:BasicMaster.ToolBarArea>
        <WrapPanel>
          <Button Style="{StaticResource SearchButton}" controls:PermissionService.FunctionId="BP0503040001" Command="{Binding SearchCommand}" />
        </WrapPanel>
      </uiLib:BasicMaster.ToolBarArea>
      <uiLib:BasicMaster.MainContent>
        <TabControl x:Name="LayoutRoot">
          <TabControl.BindingGroup>
            <BindingGroup Name="errorGroup" />
          </TabControl.BindingGroup>
          <TabItem Header="{x:Static res:Resources.Main}" Width="Auto">
            <DockPanel>
              <ContentControl Visibility="Collapsed" Content="{StaticResource ProxyElement}"></ContentControl>
              <GroupBox Header="{x:Static res:Resources.QueryCondition}" DockPanel.Dock="Top">
                <Grid>
                  <Grid.RowDefinitions>
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                  </Grid.RowDefinitions>
                  <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                  </Grid.ColumnDefinitions>
                  <Label Grid.Row="0" Grid.Column="0" Content="{x:Static res:Resources.Dc}" Style="{StaticResource LbTitleStyle}" />
                  <ComboBox Grid.Row="0" Grid.Column="1" HorizontalAlignment="Left" Style="{StaticResource ComboStyle}" ItemsSource="{Binding DcList, UpdateSourceTrigger=PropertyChanged}" DisplayMemberPath="Name" SelectedValuePath="Value" SelectedValue="{Binding SelectedDc,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                  <Label Grid.Row="0" Grid.Column="2" Content="{x:Static Properties:Resources.OrderType}" Style="{StaticResource LbTitleStyle}" />
                  <ComboBox Grid.Row="0" Grid.Column="3" HorizontalAlignment="Left" Style="{StaticResource ComboStyle}" ItemsSource="{Binding ORDTypeList, UpdateSourceTrigger=PropertyChanged}" DisplayMemberPath="Name" SelectedValuePath="Value" SelectedValue="{Binding SelectedORDType,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                  <Label Grid.Row="0" Grid.Column="4" Content="{x:Static Properties:Resources.SOURCE_NAME}" Style="{StaticResource LbTitleStyle}" />
                  <ComboBox Grid.Row="0" Grid.Column="5" HorizontalAlignment="Left" Style="{StaticResource ComboStyle}" Width="200" ItemsSource="{Binding SourceTypeList, UpdateSourceTrigger=PropertyChanged}" DisplayMemberPath="Name" SelectedValuePath="Value" SelectedValue="{Binding SelectedSourceType,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"/>
                  <Label Grid.Row="1" Grid.Column="0" Content="{x:Static Properties:Resources.ORD_DATE_FROM}" HorizontalAlignment="Right" Style="{StaticResource LbTitleStyle}" />
                  <DatePicker Grid.Row="1" Grid.Column="1" SelectedDate="{Binding OrdSDate,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Style="{StaticResource DateStyle}" />
                  <Label Content="{x:Static Properties:Resources.ORD_DATE_TO}" Grid.Row="1" Grid.Column="2" HorizontalAlignment="Right" Style="{StaticResource LbStyle}" />
                  <DatePicker Grid.Row="1" Grid.Column="3" SelectedDate="{Binding OrdEDate,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Style="{StaticResource DateStyle}" />
                  <Label Grid.Row="1" Grid.Column="4" Content="{x:Static Properties:Resources.P050304_FAST_DEAL_TYPE}" Style="{StaticResource LbTitleStyle}" />
                  <ComboBox Grid.Row="1" Grid.Column="5" HorizontalAlignment="Left" Style="{StaticResource ComboStyle}" ItemsSource="{Binding FastDealTypeList, UpdateSourceTrigger=PropertyChanged}" DisplayMemberPath="Name" SelectedValuePath="Value" SelectedValue="{Binding SelectFastDealType,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                  <Label Grid.Row="2" Grid.Column="0" Content="{x:Static Properties:Resources.ARRIVE_DATE_FROM}" HorizontalAlignment="Right" Style="{StaticResource LbTitleStyle}" />
                  <DatePicker Grid.Row="2" Grid.Column="1" SelectedDate="{Binding ArrivalSDate,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Style="{StaticResource DateStyle}" />
                  <Label Content="{x:Static Properties:Resources.ORD_DATE_TO}" Grid.Row="2" Grid.Column="2" HorizontalAlignment="Right" Style="{StaticResource LbStyle}" />
                  <DatePicker Grid.Row="2" Grid.Column="3" SelectedDate="{Binding ArrivalEDate,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Style="{StaticResource DateStyle}" />
                  <Label Grid.Row="2" Grid.Column="4" Content="{x:Static Properties:Resources.P050304_CUST_COST}" Style="{StaticResource LbTitleStyle}" />
                  <ComboBox Grid.Row="2" Grid.Column="5" HorizontalAlignment="Left" Style="{StaticResource ComboStyle}" ItemsSource="{Binding CustCostList, UpdateSourceTrigger=PropertyChanged}" DisplayMemberPath="Name" SelectedValuePath="Value" SelectedValue="{Binding SelectCustCost,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                  <Label Grid.Row="2" Grid.Column="6" Content="{x:Static Properties:Resources.P050304_CROSS_CODE}" Style="{StaticResource LbTitleStyle}" />
                  <ComboBox Grid.Row="2" Grid.Column="7" HorizontalAlignment="Left" Style="{StaticResource ComboStyle}" ItemsSource="{Binding CrossCodeList, UpdateSourceTrigger=PropertyChanged}" DisplayMemberPath="Name" SelectedValuePath="Value" SelectedValue="{Binding SelectCrossCode,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" IsEnabled="{Binding CrossCodeEnable,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"/>
                  <!--<TextBox Grid.Row="2" Grid.Column="5" MaxLength="10" Text="{Binding CUST_COST, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"  HorizontalAlignment="Left" >
                                        <TextBox.Style>
                                            <Style TargetType="TextBox" BasedOn="{StaticResource TxtStyle}">
                                                <Setter Property="MaxLength" Value="10" />
                                            </Style>
                                        </TextBox.Style>
                                    </TextBox>-->
                  <Label Grid.Row="3" Grid.Column="0" Content="{x:Static Properties:Resources.SearchORD_NO}" Style="{StaticResource LbTitleStyle}" />
                  <TextBox Grid.Row="3" Grid.Column="1" MaxLength="20" Text="{Binding ORD_NO, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"  HorizontalAlignment="Left" >
                    <TextBox.Style>
                      <Style TargetType="TextBox" BasedOn="{StaticResource TxtStyle}">
                        <Setter Property="MaxLength" Value="20" />
                      </Style>
                    </TextBox.Style>
                  </TextBox>
                  <Label Grid.Row="3" Grid.Column="2" Content="{x:Static Properties:Resources.SearchCUST_ORD_NO}" Style="{StaticResource LbTitleStyle}" />
                  <TextBox Grid.Row="3" Grid.Column="3" MaxLength="20" Text="{Binding CUST_ORD_NO, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"  HorizontalAlignment="Left" >
                    <TextBox.Style>
                      <Style TargetType="TextBox" BasedOn="{StaticResource TxtStyle}">
                        <Setter Property="MaxLength" Value="20" />
                      </Style>
                    </TextBox.Style>
                  </TextBox>
                  <Label Grid.Row="3" Grid.Column="4" Content="{x:Static Properties:Resources.P05030201_CONSIGNEE}" Style="{StaticResource LbTitleStyle}" />
                  <TextBox Grid.Row="3" Grid.Column="5" MaxLength="50" Text="{Binding CONSIGNEE, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"  HorizontalAlignment="Left" >
                    <TextBox.Style>
                      <Style TargetType="TextBox" BasedOn="{StaticResource TxtStyle}">
                        <Setter Property="MaxLength" Value="88" />
                      </Style>
                    </TextBox.Style>
                  </TextBox>
                  <Label Grid.Row="3" Grid.Column="6" Content="通路類型" Style="{StaticResource LbTitleStyle}"/>
                  <ComboBox Grid.Row="3" Grid.Column="7" ItemsSource="{Binding CHANNEL_LIST,UpdateSourceTrigger=PropertyChanged}" SelectedValuePath="Value" DisplayMemberPath="Name" SelectedValue="{Binding SelectedChannel,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Style="{StaticResource ComboStyle}"/>
                  <Label Grid.Row="3" Grid.Column="8" Content="子通路類型" Style="{StaticResource LbTitleStyle}"/>
                  <ComboBox Grid.Row="3" Grid.Column="9" ItemsSource="{Binding SUBCHANNEL_LIST,UpdateSourceTrigger=PropertyChanged}" SelectedValuePath="Value" DisplayMemberPath="Name" SelectedValue="{Binding SelectedSubChannel,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Style="{StaticResource ComboStyle}"/>
                  <Label Grid.Row="4" Grid.Column="0" Content="{x:Static Properties:Resources.P050304_RETAIL_CODE}" Style="{StaticResource LbTitleStyle}" Visibility="{Binding OrderType, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource EnumToVisibilityConverter},ConverterParameter={x:Static viewModel:OrderType.B2B}}" />
                  <TextBox Grid.Row="4" Grid.Column="1" MaxLength="50" Text="{Binding RETAIL_CODE, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"   HorizontalAlignment="Left" Visibility="{Binding OrderType, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource EnumToVisibilityConverter},ConverterParameter={x:Static viewModel:OrderType.B2B}}" >
                    <TextBox.Style>
                      <Style TargetType="TextBox" BasedOn="{StaticResource TxtStyle}">
                        <Setter Property="MaxLength" Value="20" />
                      </Style>
                    </TextBox.Style>
                  </TextBox>
                  <Label Grid.Row="4" Grid.Column="2" Content="{x:Static Properties:Resources.TAKE_TIME}" Style="{StaticResource LbTitleStyle}" Visibility="{Binding OrderType, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource EnumToVisibilityConverter},ConverterParameter={x:Static viewModel:OrderType.B2B}}" />
                  <ComboBox Grid.Row="4" Grid.Column="3" HorizontalAlignment="Left" Style="{StaticResource ComboStyle}" ItemsSource="{Binding CAR_PERIODList, UpdateSourceTrigger=PropertyChanged}" DisplayMemberPath="Name" SelectedValuePath="Value" SelectedValue="{Binding SelectedCAR_PERIOD,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" 
                                               Visibility="{Binding OrderType, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource EnumToVisibilityConverter},ConverterParameter={x:Static viewModel:OrderType.B2B}}"  />
                  <!--<TextBox Grid.Row="4" Grid.Column="3" MaxLength="50" Text="{Binding CAR_PERIOD, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"   HorizontalAlignment="Left" Visibility="{Binding OrderType, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource EnumToVisibilityConverter},ConverterParameter={x:Static viewModel:OrderType.B2B}}" >
                    <TextBox.Style>
                      <Style TargetType="TextBox" BasedOn="{StaticResource TxtStyle}">
                        <Setter Property="MaxLength" Value="2" />
                      </Style>
                    </TextBox.Style>
                  </TextBox>-->
                  <Label Grid.Row="4" Grid.Column="4" Foreground="Blue" Content="{x:Static Properties:Resources.DELV_NO}" Style="{StaticResource LbTitleStyle}" Visibility="{Binding OrderType, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource EnumToVisibilityConverter},ConverterParameter={x:Static viewModel:OrderType.B2B}}" />
                  <TextBox Grid.Row="4" Grid.Column="5" MaxLength="50" Text="{Binding DELV_NO, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"  HorizontalAlignment="Left" Visibility="{Binding OrderType, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource EnumToVisibilityConverter},ConverterParameter={x:Static viewModel:OrderType.B2B}}" >
                    <TextBox.Style>
                      <Style TargetType="TextBox" BasedOn="{StaticResource TxtStyle}">
                        <Setter Property="MaxLength" Value="10" />
                      </Style>
                    </TextBox.Style>
                  </TextBox>
                  <views:UcSearchProduct Grid.Row="5" Grid.RowSpan="2" Grid.Column="0" Grid.ColumnSpan="6" Margin="70,0,0,0" 
                                         ItemCode="{Binding DataContext.ITEM_CODE, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged,Source={StaticResource ProxyElement}}" 
                                         HasItem="{Binding DataContext.SearchHasItem, Mode=OneWayToSource, UpdateSourceTrigger=PropertyChanged,Source={StaticResource ProxyElement}}" 
                                         ItemName="{Binding DataContext.ITEM_NAME, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged,Source={StaticResource ProxyElement}}" 
                                         SerialNo="{Binding DataContext.SERIAL_NO, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged,Source={StaticResource ProxyElement}}" 
                                         ItemColorVisibility="Collapsed" ItemSpecVisibility="Collapsed" ItemSizeVisibility="Collapsed">
                  </views:UcSearchProduct>
                </Grid>
              </GroupBox>
              <GroupBox Header="{x:Static Properties:Resources.QueryResult}">
                <DockPanel>
                  <WrapPanel DockPanel.Dock="Top">
                    <!--<CheckBox Content="{x:Static Properties:Resources.BATCH_NO}" IsChecked="{Binding IsBatch,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Style="{StaticResource CheckStyle}" />
										<TextBox  Height="35" HorizontalAlignment="Left" Text="{Binding BATCH_NO, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" >
											<TextBox.Style>
												<Style TargetType="TextBox" BasedOn="{StaticResource TxtStyle}">
													<Setter Property="MaxLength" Value="50" />
												</Style>
											</TextBox.Style>
										</TextBox>-->
                    <Button Content="{x:Static Properties:Resources.IsBatch}" Command="{Binding Allocation}" Height="40" Width="120" Margin="10,0,0,0" controls:PermissionService.FunctionId="BP0503040002" />
                    <Button Content="{x:Static Properties:Resources.P0503040000_Calculation}" Command="{Binding CalculationCommand}" Height="40" Width="120" Margin="40,0,0,0" controls:PermissionService.FunctionId="BP0503040003" />
										<Label  Content="指定自動倉揀貨優先旗標" Style="{StaticResource LbTitleStyle}" />
										<ComboBox  Style="{StaticResource ComboStyle}" ItemsSource="{Binding PriorityList, UpdateSourceTrigger=PropertyChanged}" DisplayMemberPath="Name" SelectedValuePath="Value" 
															 SelectedValue="{Binding SelectedPriorityCode,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"  />

									</WrapPanel>
                  <DataGrid Style="{StaticResource DgStyle}" SelectionMode="Single" ItemsSource="{Binding SerialRecords,UpdateSourceTrigger=PropertyChanged}" SelectedItem="{Binding SelectSerialRecord, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" IsReadOnly="False" CellStyle="{StaticResource UcSingleRowEdit}" RowHeaderStyle="{StaticResource UcSingleRowHeaderEdit}">
                    <DataGrid.Columns>
                      <DataGridCheckBoxColumn Binding="{Binding IsSelected}">
                        <DataGridCheckBoxColumn.HeaderTemplate>
                          <DataTemplate>
                            <CheckBox Content="{x:Static res:Resources.SelectAll}" IsChecked="{Binding DataContext.IsCheckAll, RelativeSource={RelativeSource FindAncestor, AncestorType=UserControl},Mode = TwoWay,UpdateSourceTrigger=PropertyChanged}"></CheckBox>
                          </DataTemplate>
                        </DataGridCheckBoxColumn.HeaderTemplate>
                      </DataGridCheckBoxColumn>
                      <DataGridTextColumn Header="{x:Static res:Resources.ItemNumber}" IsReadOnly="True">
                        <DataGridTextColumn.Binding>
                          <MultiBinding Converter="{StaticResource DataGridRowNumberConverter}">
                            <Binding />
                            <Binding RelativeSource="{RelativeSource FindAncestor,AncestorType=DataGrid}" />
                          </MultiBinding>
                        </DataGridTextColumn.Binding>
                      </DataGridTextColumn>
                      <DataGridTextColumn Header="{x:Static Properties:Resources.ORD_DATE_FROM}" IsReadOnly="True" Binding="{Binding Item.ORD_DATE,Converter={StaticResource DateTimeToStringConverter}, ConverterParameter=yyyy/MM/dd}" />
                      <DataGridTextColumn Header="{x:Static Properties:Resources.ARRIVE_DATE_FROM}" IsReadOnly="True" Binding="{Binding Item.ARRIVAL_DATE,Converter={StaticResource DateTimeToStringConverter}, ConverterParameter=yyyy/MM/dd}" />
                      <DataGridTextColumn Header="{x:Static Properties:Resources.SearchORD_NO}" IsReadOnly="True" Binding="{Binding Item.ORD_NO}" />
                      <DataGridTextColumn Header="{x:Static Properties:Resources.SearchCUST_ORD_NO}" IsReadOnly="True" Binding="{Binding Item.CUST_ORD_NO}" />
                      <DataGridTextColumn Header="{x:Static Properties:Resources.SOURCE_NAME}" IsReadOnly="True" Binding="{Binding Item.SOURCE_NAME}" />
                      <DataGridTextColumn Header="{x:Static Properties:Resources.SelectedDelvType}" IsReadOnly="True" Binding="{Binding Item.DELV_TYPE}" Visibility="{Binding DataContext.OrderType,Source={StaticResource ProxyElement}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource EnumToVisibilityConverter},ConverterParameter={x:Static viewModel:OrderType.B2C}}" />
                      <DataGridTextColumn Header="{x:Static Properties:Resources.P050304_RETAIL_CODE}" IsReadOnly="True" Binding="{Binding Item.RETAIL_CODE}" Visibility="{Binding DataContext.OrderType,Source={StaticResource ProxyElement}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource EnumToVisibilityConverter},ConverterParameter={x:Static viewModel:OrderType.B2B}}" />
                      <DataGridTextColumn Header="{x:Static Properties:Resources.CUST_NAME}" IsReadOnly="True" Binding="{Binding Item.RETAIL_NAME}" Visibility="{Binding DataContext.OrderType,Source={StaticResource ProxyElement}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource EnumToVisibilityConverter},ConverterParameter={x:Static viewModel:OrderType.B2B}}" />
                      <DataGridTextColumn Header="{x:Static Properties:Resources.TAKE_TIME}" IsReadOnly="True" Binding="{Binding Item.CAR_PERIOD}" Visibility="{Binding DataContext.OrderType,Source={StaticResource ProxyElement}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource EnumToVisibilityConverter},ConverterParameter={x:Static viewModel:OrderType.B2B}}" />
                      <DataGridTextColumn Header="{x:Static Properties:Resources.DELV_NO}" IsReadOnly="True" Binding="{Binding Item.DELV_NO}" Visibility="{Binding DataContext.OrderType,Source={StaticResource ProxyElement}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource EnumToVisibilityConverter},ConverterParameter={x:Static viewModel:OrderType.B2B}}" />
                      <DataGridTextColumn Header="{x:Static Properties:Resources.DELV_WAY}" IsReadOnly="True" Binding="{Binding Item.DELV_WAY}" Visibility="{Binding DataContext.OrderType,Source={StaticResource ProxyElement}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource EnumToVisibilityConverter},ConverterParameter={x:Static viewModel:OrderType.B2B}}" />
                      <DataGridTextColumn Header="{x:Static Properties:Resources.CUST_COST}" IsReadOnly="True"  Visibility="{Binding DataContext.OrderType,Source={StaticResource ProxyElement}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource EnumToVisibilityConverter},ConverterParameter={x:Static viewModel:OrderType.B2C}}">
                        <DataGridTextColumn.Binding>
                          <MultiBinding Converter="{StaticResource NameValuePairMultiConverter}">
                            <Binding Path="Item.CUST_COST" />
                            <Binding Path="DataContext.CustCostList" RelativeSource="{RelativeSource AncestorType={x:Type uiLib:Wms3plUserControl}}" />
                          </MultiBinding>
                        </DataGridTextColumn.Binding>
                      </DataGridTextColumn>
                      <DataGridTextColumn Header="{x:Static Properties:Resources.FAST_DEAL_TYPE}" IsReadOnly="True" Visibility="{Binding DataContext.OrderType,Source={StaticResource ProxyElement}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource EnumToVisibilityConverter},ConverterParameter={x:Static viewModel:OrderType.B2C}}" >
                        <DataGridTextColumn.Binding>
                          <MultiBinding Converter="{StaticResource NameValuePairMultiConverter}">
                            <Binding Path="Item.FAST_DEAL_TYPE" />
                            <Binding Path="DataContext.FastDealTypeList" RelativeSource="{RelativeSource AncestorType={x:Type uiLib:Wms3plUserControl}}" />
                          </MultiBinding>
                        </DataGridTextColumn.Binding>
                      </DataGridTextColumn>
                      <DataGridTextColumn Header="{x:Static Properties:Resources.MOVE_OUT_TARGET}" IsReadOnly="True" Binding="{Binding Item.MOVE_OUT_TARGET}"  Visibility="{Binding DataContext.OrderType,Source={StaticResource ProxyElement}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource EnumToVisibilityConverter},ConverterParameter={x:Static viewModel:OrderType.B2C}}">
                      </DataGridTextColumn>
                      <DataGridTextColumn Header="{x:Static Properties:Resources.CRT_DATE}" IsReadOnly="True" Binding="{Binding Item.CRT_DATE,StringFormat=\{0:yyyy/MM/dd HH:mm:ss\}}" />
                      <DataGridTextColumn Header="通路類型" IsReadOnly="True" Binding="{Binding Item.CHANNEL}" Visibility="{Binding DataContext.OrderType,Source={StaticResource ProxyElement}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource EnumToVisibilityConverter},ConverterParameter={x:Static viewModel:OrderType.B2C}}"/>
                      <DataGridTextColumn Header="子通路類型" IsReadOnly="True" Binding="{Binding Item.SUBCHANNEL}" Visibility="{Binding DataContext.OrderType,Source={StaticResource ProxyElement}, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource EnumToVisibilityConverter},ConverterParameter={x:Static viewModel:OrderType.B2C}}"/>
                    </DataGrid.Columns>
                  </DataGrid>
                </DockPanel>
              </GroupBox>
            </DockPanel>
          </TabItem>
        </TabControl>
      </uiLib:BasicMaster.MainContent>
    </uiLib:BasicMaster>
  </telerik:RadBusyIndicator>
</uiLib:Wms3plUserControl>